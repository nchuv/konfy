    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-lazy": "0.0.4",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "postcss-preset-env": "^6.7.0",
