  "main": "index.js",
  "scripts": {
    "predev": "rm -rf dist/*",
    "dev": "parcel ./src/index.pug --no-cache --port 9999 --open",
    "prebuild": "rm -rf public/*",
    "build": "NODE_ENV=production parcel build ./src/index.pug --public-url . --out-dir ./public --no-cache",
    "postbuild": "cp ./src/static/robots.txt ./public/; cp ./src/static/humans.txt ./public/; echo 'âœ¨  Build complete. '"
  },
  "staticFiles": {
    "watcherGlob": "**"
  },
